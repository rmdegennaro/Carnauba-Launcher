{
  "name": "Carnauba Launcher",
  "description": "A way to oraganize & launch apps on non-ChromeOS machines.",
  "version": "0.0.0.1",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "icons": { "16": "carnauba-16.png", "128": "carnauba-128.png" },
  "permissions": [
    "identity"
  ]
}
